{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Azure": {
    "Environment": "Dev",
    "SubscriptionId": "<dev-subscription-id>",
    "TenantId": "<dev-tenant-id>",
    "ResourceGroup": "<dev-resource-group>",
    "AppServiceName": "<dev-app-service-name>",
    "KeyVaultName": "<dev-keyvault-name>",
    "StorageAccount": "<dev-storage-account>",
    "KeyVaultUri": "<dev-keyvault-uri>"
  },
  "AzureAppConfiguration": {
    "ConnectionString": "<dev-app-configuration-connection-string>"
  },
  "Jwt": {
    "Issuer": "wpfdemo-dev",
    "Audience": "wpfdemo-dev",
    "Key": "<dev-jwt-signing-key>"
  },
  "Cors": {
    "AllowedOrigins": [ "https://localhost:7042", "http://localhost:5092" ]
  },
  "RateLimiting": {
    "PermitLimit": 120,
    "WindowSeconds": 60,
    "QueueLimit": 0
  },
  "FeatureManagement": {
    "EnableDelete": true
  },
  "Users": [
    {
      "UserName": "admin",
      "Password": "Admin@123",
      "Role": "Admin"
    },
    {
      "UserName": "user",
      "Password": "User@123",
      "Role": "User"
    }
  ]
}
